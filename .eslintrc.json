{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "global-require": 0,
        "react/require-default-props": 0,
        "react/prefer-stateless-function": 1,
        "no-underscore-dangle": 1
    }
}